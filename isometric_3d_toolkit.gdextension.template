[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://addons/godot-rust-toolkit/target/debug/libgodot_rust_isometric_3d_toolkit.so"
linux.release.x86_64 =   "res://addons/godot-rust-toolkit/target/release/libgodot_rust_isometric_3d_toolkit.so"
windows.debug.x86_64 =   "res://addons/godot-rust-toolkit/target/debug/{YourCrate}.dll"
windows.release.x86_64 = "res://addons/godot-rust-toolkit/target/release/{YourCrate}.dll"
macos.debug =            "res://addons/godot-rust-toolkit/target/debug/libgodot_rust_isometric_3d_toolkit.dylib"
macos.release =          "res://addons/godot-rust-toolkit/target/release/libgodot_rust_isometric_3d_toolkit.dylib"
macos.debug.arm64 =      "res://addons/godot-rust-toolkit/target/debug/libgodot_rust_isometric_3d_toolkit.dylib"
macos.release.arm64 =    "res://addons/godot-rust-toolkit/target/release/libgodot_rust_isometric_3d_toolkit.dylib"
